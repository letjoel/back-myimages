{
  "version": 2,
  "env": {
    "MONGODB_URL": "@MONGODB_URL",
    "PORT": "@PORT",
    "HOST_API": "@HOST_API"
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
